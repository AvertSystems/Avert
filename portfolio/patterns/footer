<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>
<script>
   $( window ).on("load", function() {
      /*var sm-item-style = {flexBasis: "100%",backgroundColor: "black",color: "white",textAlign: "center",borderBottom: "2px solid #8F37FA"
      };
      var sm-menu-style = {display: "flex",flexDirection: "column",position: "absolute",top: "50px"};*/
      var vpw = $( window ).width();
      if (vpw < 600) {
         //$("#sillyFooter").toggle();
         $(".wp-block-site-title").css("margin","auto");
         $("#ft-tagline").css("position","relative");
         $("#ft-title").css("position","relative");
         $("#ft-tagline").css("left","30%");
         $("#ft-title").css("left","15%");
         $("#sm-footer").css({"display":"block","width":"100%","position":"relative","top":"-30px","left":"0px"});
         $(".sm-item").css({"flex-basis":"100%","background-color":"black","color":"white","text-align":"center","border-bottom":"2px solid #8F37FA"});   
      } else if (vpw >= 600 && vpw < 768) {
         $(".sillyFooter").show();
         $("#sm-footer").hide();
      } else if (vpw >= 768 && vpw < 992) {
         $(".sillyFooter").show();
         $("#sm-footer").hide();
      } else if (vpw >= 992 && vpw < 1200) {
         $(".sillyFooter").show();
         $("#sm-footer").hide();
      } else {
         $(".sillyFooter").show();
      }
   });
   $( window ).on("resize", function() {
      /*var sm-item-style = {flexBasis: "100%",backgroundColor: "black",color: "white",textAlign: "center",borderBottom: "2px solid #8F37FA"
      };
      var sm-menu-style = {display: "flex",flexDirection: "column",position: "absolute",top: "50px"};*/
      var vpw = $( window ).width();
      if (vpw < 600) {
         $(".sillyFooter").hide();
         $(".wp-block-site-title").css("margin","auto");
         $("#ft-tagline").css("position","relative");
         $("#ft-title").css("position","relative");
         //$("#ft-tagline").css("left","30%");
         //$("#ft-title").css("left","15%");
         $("#sm-footer").css({"display":"block","width":"100%","position":"relative","top":"-30px","left":"0px"});
         $(".sm-item").css({"flex-basis":"100%","background-color":"black","color":"white","text-align":"center","border-bottom":"2px solid #8F37FA"});   
      } else if (vpw >= 600 && vpw < 768) {
         //$(".wp-block-site-title").css("margin","none");
         $(".sillyFooter").show();
         $("#sm-footer").hide();
      } else if (vpw >= 768 && vpw < 992) {
         //$(".wp-block-site-title").css("margin","auto auto auto 0px");
         $(".sillyFooter").show();
         $("#sm-footer").hide();
      } else if (vpw >= 992 && vpw < 1200) {
         //$(".wp-block-site-title").css("margin","auto auto auto 0px");
         $(".sillyFooter").show();
         $("#sm-footer").hide();
      } else {
         //$(".wp-block-site-title").css("margin","auto auto auto 0px");
         $(".sillyFooter").show();
      }
   });
</script>
